package pe.nasqa.values.model.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class ExportBaseCourier implements Serializable{
	
	@Id
	private String id_distribucion;
	@Column
	private String codigo_barra;
	@Column
	private String hoja_ruta;
	@Column
	private String fecha_hoja;
	@Column
	private String titular;
	@Column
	private String direccion_basica;
	@Column
	private String direccion_alterna_1;
	@Column
	private String direccion_alterna_2;
	@Column
	private String direccion_alterna_3;
	
	public String getId_distribucion() {
		return id_distribucion;
	}
	public void setId_distribucion(String id_distribucion) {
		this.id_distribucion = id_distribucion;
	}
	public String getCodigo_barra() {
		return codigo_barra;
	}
	public void setCodigo_barra(String codigo_barra) {
		this.codigo_barra = codigo_barra;
	}
	public String getHoja_ruta() {
		return hoja_ruta;
	}
	public void setHoja_ruta(String hoja_ruta) {
		this.hoja_ruta = hoja_ruta;
	}
	public String getFecha_hoja() {
		return fecha_hoja;
	}
	public void setFecha_hoja(String fecha_hoja) {
		this.fecha_hoja = fecha_hoja;
	}
	public String getTitular() {
		return titular;
	}
	public void setTitular(String titular) {
		this.titular = titular;
	}
	public String getDireccion_basica() {
		return direccion_basica;
	}
	public void setDireccion_basica(String direccion_basica) {
		this.direccion_basica = direccion_basica;
	}
	public String getDireccion_alterna_1() {
		return direccion_alterna_1;
	}
	public void setDireccion_alterna_1(String direccion_alterna_1) {
		this.direccion_alterna_1 = direccion_alterna_1;
	}
	public String getDireccion_alterna_2() {
		return direccion_alterna_2;
	}
	public void setDireccion_alterna_2(String direccion_alterna_2) {
		this.direccion_alterna_2 = direccion_alterna_2;
	}
	public String getDireccion_alterna_3() {
		return direccion_alterna_3;
	}
	public void setDireccion_alterna_3(String direccion_alterna_3) {
		this.direccion_alterna_3 = direccion_alterna_3;
	}
	
}
